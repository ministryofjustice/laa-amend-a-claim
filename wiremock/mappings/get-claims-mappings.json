{
  "request": {
    "method": "GET",
    "urlPath": "/api/v0/claims",
    "queryParameters": {
      "office_code": {
        "matches": ".*"
      },
      "size": {
        "equalTo": "10"
      },
      "page": {
        "matches": ".*"
      }
    }
  },
  "response": {
    "status": 200,
    "bodyFileName": "get-claims-response.json",
    "transformers": ["response-template"],
    "headers": {
      "Content-Type": "application/json"
    }
  }
}